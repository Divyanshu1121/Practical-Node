<% include('layout', { title: 'Articles' }); %>
    <h1>Articles</h1>
    <ul>
        <% articles.forEach(article=> { %>
            <li>
                <h2>
                    <%= article.title %>
                </h2>
                <p>
                    <%= article.content %>
                </p>
                <p><small>By <%= article.author.username %></small></p>
            </li>
            <% }); %>
    </ul>